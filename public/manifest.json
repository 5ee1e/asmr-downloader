{
  "name": "ASMR Download",
  "description": "Download Kikoeru files in one click!",
  "version": "1.0.0",
  "manifest_version": 3,
  "permissions": [
    "downloads"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://www.asmr.one/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}